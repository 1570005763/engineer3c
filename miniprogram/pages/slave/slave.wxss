.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  margin: 15rpx;
} 

.camera{
  position: relative;
  top: 0rpx;
  width: 100%;
  height: 900rpx;
}

navigator {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  margin: 30rpx;
  border: 1px solid rgba(0, 0, 0, 0.1);
}
